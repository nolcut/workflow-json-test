{
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "nolcut",
            "UseSecretStore": true,
            "ActionRepoName": "FaaSr-tutorial-py",
            "Branch": "main"
        }
    },
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "faasr",
            "Region": "us-east-1",
            "Writable": "TRUE"
        }
    },
    "ActionList": {
        "start": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_GitHub_Account",
            "Type": "R",
            "Arguments": {
                "folder": "tutorial",
                "output1": "sample1.csv",
                "output2": "sample2.csv"
            },
            "InvokeNext": ["sum"]
        },
        "sum": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_GitHub_Account",
            "Type": "R",
            "Arguments": {
                "folder": "tutorial",
                "input1": "sample1.csv",
                "input2": "sample2.csv",
                "output": "sum.csv"
            },
            "InvokeNext": [
            ]
        }
    },
    "ActionContainers": {
        "start": "ghcr.io/faasr/github-actions-r:dev",
        "sum": "ghcr.io/faasr/github-actions-r:dev"
    },
    "FunctionGitRepo": {
        "create_sample_data": "FaaSr/FaaSr-tutorial",
        "compute_sum": "FaaSr/FaaSr-tutorial"
    },
    "WorkflowName": "TUTORIAL",
    "FunctionInvoke": "start",
    "InvocationID": "",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}

